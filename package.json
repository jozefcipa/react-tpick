  "main": "dist/index.cjs.js",
  "module": "dist/index.esm.js",
  "repository": {
    "url": "https://github.com/jozefcipa/react-tpick",
    "type": "git"
  },
  "author": {
    "name": "Jozef Cipa",
    "email": "cipajozef@gmail.com",
